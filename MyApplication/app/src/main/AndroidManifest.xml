<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.grsu.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/name_app">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryActivity"
            android:label="@string/name_categ"/>
        <activity android:name=".CategoryCardActivity"
            android:label="@string/name_categcard"/>
        <activity android:name=".IncomeActivity"
            android:label="@string/name_inc"/>
        <activity android:name=".ICardActivity"
            android:label="@string/name_inccard"/>
        <activity android:name=".OutlayActivity"
            android:label="@string/name_out"/>
        <activity android:name=".ReportActivity"
            android:label="@string/name_report"/>
        <activity android:name=".OCardActivity"
            android:label="@string/name_outcard"></activity>
    </application>

</manifest>